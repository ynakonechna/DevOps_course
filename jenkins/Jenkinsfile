pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                withAWS(role:"jenkins", roleSessionName: "role", useNode: true){
                    sh "aws --version"
                }
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deplying...'
            }
        }
    }
}
